# Ignorar arquivos de linguagens que não são Dart
* linguagens=none

# Configura o Git para rastrear apenas arquivos Dart
*.dart linguagens=dart

# Especificar extensões de arquivos para Dart
*.dart linguagens=dart
*.yaml linguagens=dart

# Ignorar arquivos e diretórios específicos
.gradle linguagens=none
build/ linguagens=none
.idea/ linguagens=none
.vscode/ linguagens=none
*.iml linguagens=none
*.lock linguagens=none

# Configurações para outros arquivos temporários ou específicos do ambiente
*.log linguagens=none
*.tmp linguagens=none
